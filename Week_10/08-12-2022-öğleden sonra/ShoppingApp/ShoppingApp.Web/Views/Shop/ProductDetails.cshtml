@using ShoppingApp.Web.Models.Dtos
@model ProductDetailsDto

<div>@Model.Id</div>
<div>@Model.Name</div>
<div>@Model.Price</div>
<div>@Model.ImageUrl</div>
<div>@Model.Url</div>
<div>@Model.Description</div>

<div>
    Kategoriler:
    @foreach (var category in Model.Categories)
    {
        <div>@category.Name</div>
    }

</div>